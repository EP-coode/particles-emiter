{
  "compilerOptions": {
    "incremental": true,
    "target": "ES5",
    "module": "CommonJS",
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules/**", "src/examples/*"],
  "compileOnSave": false
}
