{
  "name": "@ep44/particles-emmiter",
  "version": "0.0.2",
  "type": "module",
  "keywords": [
    "particles",
    "emmiter",
    "playground"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EP-coode/particles-emmiter"
  },
  "publishConfig": {
    "access": "public",
    "@ep44:particles-emmiter": "https://www.npmjs.com/package/"
  },
  "main": "ParticlesEmmiter.cjs",
  "types": "./index.d.ts",
  "module": "./dist/ParticlesEmmiter.js",
  "exports": {
    ".": {
      "import": "./dist/ParticlesEmmiter.js",
      "require": "./dist/ParticlesEmmiter.cjs",
      "type": "./dist/ParticlesEmmiter.d.ts"
    },
    "./SizeChange": {
      "import": "./dist/SizeChange.js",
      "require": "./dist/SizeChange.cjs",
      "type": "./dist/SizeChange.d.ts"
    },
    "./ColorSelection": {
      "import": "./dist/ColorSelection.js",
      "require": "./dist/ColorSelection.cjs",
      "type": "./dist/ColorSelection.d.ts"
    }
  },
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.11.12",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1"
  }
}
